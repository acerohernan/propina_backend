{
  "name": "propina_backend",
  "version": "1.0.0",
  "description": "This is a backend for Propina.pe",
  "main": "index.js",
  "engines": {
    "node": "16.16.0",
    "npm": "please-use-yarn",
    "yarn": "1.22.19"
  },
  "scripts": {
    "dev": "export NODE_ENV=development&& ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "tsc && export NODE_ENV=production&& node build/src/server.js",
    "pre-commit": "yarn lint&& yarn prettier-format",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [],
  "author": "acerohernan",
  "license": "MIT",
  "devDependencies": {
    "@types/config": "^3.3.0",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "config": "^3.3.7",
    "dayjs": "^1.11.4",
    "dotenv": "^16.0.1",
    "pino": "^8.3.1",
    "pino-pretty": "^8.1.0",
    "ts-node-dev": "^2.0.0"
  }
}
